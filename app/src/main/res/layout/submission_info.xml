<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textViewContestId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="1621"
        app:layout_constraintBottom_toTopOf="@+id/textViewCreationTime"
        app:layout_constraintEnd_toStartOf="@+id/textViewIndex"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewCreationTime"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="12/24/2020"
        app:layout_constraintBottom_toTopOf="@+id/textViewVerdict"
        app:layout_constraintEnd_toStartOf="@+id/textViewProblemRating"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewIndex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="8dp"
        android:text="A"
        app:layout_constraintEnd_toStartOf="@+id/textViewProblemName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewProblemName"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Stable Arrangement of Rooks"
        app:layout_constraintEnd_toStartOf="@+id/textViewLanguageUsed"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewPoints"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Points: 500"
        app:layout_constraintBottom_toTopOf="@+id/textViewVerdict"
        app:layout_constraintEnd_toStartOf="@+id/textViewTimeConsumed"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewProblemRating" />

    <TextView
        android:id="@+id/textViewProblemRating"
        android:layout_width="102dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Rating 800"
        app:layout_constraintBottom_toTopOf="@+id/textViewVerdict"
        app:layout_constraintEnd_toStartOf="@+id/textViewPoints"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewCreationTime" />

    <TextView
        android:id="@+id/textViewVerdict"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="OK"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewLanguageUsed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:text="GNU C++17"
        app:layout_constraintBottom_toTopOf="@+id/textViewTimeConsumed"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:id="@+id/textViewMemoryConsumed"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/textViewTimeConsumed"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewLanguageUsed"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewTimeConsumed"
        android:layout_width="102dp"
        android:layout_height="18dp"
        android:layout_marginBottom="8dp"
        android:text="Time 15"
        app:layout_constraintBottom_toTopOf="@+id/textViewVerdict"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewPoints" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="120dp" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="textViewCreationTime,textViewProblemRating,textViewPoints,textViewTimeConsumed" />

</androidx.constraintlayout.widget.ConstraintLayout>